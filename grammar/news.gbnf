root ::= NewspaperArticle
NewspaperArticle ::= "{"   ws   "\"title\":"   ws   string   ","   ws   "\"subtitle\":"   ws   string   ","   ws   "\"author\":"   ws   string   ","   ws   "\"date\":"   ws   string   ","   ws   "\"content\":"   ws   string   ","   ws   "\"tags\":"   ws   stringlist   ","   ws   "\"references\":"   ws   stringlist   "}"
NewspaperArticlelist ::= "[]" | "["   ws   NewspaperArticle   (","   ws   NewspaperArticle)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"
